<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <absolute-ordering/>
    <listener>
        <listener-class>com.chargebee.samples.common.EnvInitializer</listener-class>
    </listener>
    <servlet>
        <servlet-name>stripe_js_checkout</servlet-name>
        <servlet-class>com.chargebee.samples.StripeJsCheckout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stripe_js_3ds_checkout</servlet-name>
        <servlet-class>com.chargebee.samples.StripeJs3dsCheckout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stripe_js_3ds_confirm</servlet-name>
        <servlet-class>com.chargebee.samples.StripeJs3dsCheckout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkout_custom_field</servlet-name>
        <servlet-class>com.chargebee.samples.CustomFieldCheckout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>custom_field_redirect_handler</servlet-name>
        <servlet-class>com.chargebee.samples.custom_field.RedirectHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>estimate_checkout</servlet-name>
        <servlet-class>com.chargebee.samples.EstimateCheckout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>webhook_handler</servlet-name>
        <servlet-class>com.chargebee.samples.WebhookHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>self_service_portal</servlet-name>
        <servlet-class>com.chargebee.samples.SelfServicePortal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>plan_configuration</servlet-name>
        <servlet-class>com.chargebee.samples.PlanConfiguration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkout_using_iframe</servlet-name>
        <servlet-class>com.chargebee.samples.CheckoutUsingIframe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BraintreeJs</servlet-name>
        <servlet-class>com.chargebee.samples.BraintreeJs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StripePopUpJs</servlet-name>
        <servlet-class>com.chargebee.samples.StripePopUpJs</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>stripe_js_checkout</servlet-name>
        <url-pattern>/stripe_js/checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stripe_js_3ds_checkout</servlet-name>
        <url-pattern>/stripe_js_3ds/checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stripe_js_3ds_confirm</servlet-name>
        <url-pattern>/stripe_js_3ds/confirm_payment</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>trial_signup</servlet-name>
        <servlet-class>com.chargebee.samples.TrialSignup</servlet-class>
    </servlet>
