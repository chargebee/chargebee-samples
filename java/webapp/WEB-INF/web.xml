        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>plan_configuration</servlet-name>
        <url-pattern>/plan_config</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkout_using_iframe</servlet-name>
        <url-pattern>/checkout_iframe/checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkout_using_iframe</servlet-name>
        <url-pattern>/checkout_iframe/redirect_handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BraintreeJs</servlet-name>
        <url-pattern>/braintree-js/checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StripePopUpJs</servlet-name>
        <url-pattern>/stripe-popup-js/checkout</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>400</error-code>
        <location>/error_pages/400.html</location>
    </error-page>
    <error-page>
        <error-code>403</error-code>
        <location>/error_pages/403.html</location>
    </error-page>
    <error-page>
        <error-code>404</error-code>
        <location>/error_pages/404.html</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/error_pages/500.html</location>
    </error-page>
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/error_handler</location>
    </error-page>
</web-app>
