        <servlet-name>trial_signup</servlet-name>
        <url-pattern>/trial_signup/signup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>checkout_existing</servlet-name>
        <servlet-class>com.chargebee.samples.CheckoutExisting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>checkout_existing</servlet-name>
        <url-pattern>/checkout_existing/checkout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>redirect_handler</servlet-name>
        <servlet-class>com.chargebee.samples.RedirectHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>redirect_handler</servlet-name>
        <url-pattern>/checkout_existing/redirect_handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>redirect_handler</servlet-name>
        <url-pattern>/checkout_new/redirect_handler</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>checkout_new</servlet-name>
        <servlet-class>com.chargebee.samples.CheckoutNew</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>checkout_new</servlet-name>
        <url-pattern>/checkout_new/checkout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>update_payment_method</servlet-name>
        <servlet-class>com.chargebee.samples.UpdatePaymentMethod</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>update_payment_method</servlet-name>
        <url-pattern>/update_payment_method/update</url-pattern>
        <url-pattern>/update_payment_method/redirect_handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkout_custom_field</servlet-name>
        <url-pattern>/custom_field/checkout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>checkout_two_step</servlet-name>
        <servlet-class>com.chargebee.samples.CheckoutTwoStep</servlet-class>
    </servlet>
