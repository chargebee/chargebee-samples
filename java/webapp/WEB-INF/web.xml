        <servlet-name>checkout_two_step</servlet-name>
        <url-pattern>/checkout_two_step/first_step</url-pattern>
        <url-pattern>/checkout_two_step/redirect_handler</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>error_handler</servlet-name>
        <servlet-class>com.chargebee.samples.common.ErrorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>error_handler</servlet-name>
        <url-pattern>/error_handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>estimate_checkout</servlet-name>
        <url-pattern>/estimate/estimate_checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>webhook_handler</servlet-name>
        <url-pattern>/webhook_handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>estimate_checkout</servlet-name>
        <url-pattern>/estimate/order_summary</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>self_service_portal</servlet-name>
