        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/redirect_handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/sub_cancel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/update_billing_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/update_shipping_address</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/invoice_as_pdf</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/sub_reactivate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>self_service_portal</servlet-name>
        <url-pattern>/ssp/update_account_info</url-pattern>
    </servlet-mapping>
